{
  "name": "@traw/traw",
  "version": "0.1.2",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/trawhq/traw.git"
  },
  "description": "A skeleton to create your own React component library using Rollup, TypeScript, Sass and Storybook",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "TIMING=1 eslint 'src/**/*.{ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "canvas",
    "zoom",
    "drawing",
    "whiteboard",
    "collaboration",
    "tldraw"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.0.2",
    "@tldraw/tldraw": "^1.26.2",
    "classnames": "^2.3.2",
    "zustand": "^4.1.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "yalcSig": "ef802cc5ae397b379efbf49ade2259ea"
}
