{
  "name": "@traw/traw",
  "version": "0.1.3-alpha1",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/trawhq/traw.git"
  },
  "description": "A skeleton to create your own React component library using Rollup, TypeScript, Sass and Storybook",
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "TIMING=1 eslint 'src/**/*.{ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "canvas",
    "zoom",
    "drawing",
    "whiteboard",
    "collaboration",
    "tldraw"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.5.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^24.9.1",
    "@types/react": "^18.0.22",
    "@types/react-dom": "^18.0.7",
    "@types/uuid": "^9.0.0",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "8.29.0",
    "eslint-config-custom": "*",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "postcss": "^8.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^2.56.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^26.5.6",
    "tsconfig": "*",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.0.2",
    "@tldraw/tldraw": "^1.26.3",
    "classnames": "^2.3.2",
    "immer": "^9.0.16",
